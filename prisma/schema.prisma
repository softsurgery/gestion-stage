generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String
  published Boolean
}

model Intern {
  id        Int      @id @default(autoincrement())
  cin       String   @unique
  name      String
  surname   String
  organisme String
  birthdate DateTime
}
